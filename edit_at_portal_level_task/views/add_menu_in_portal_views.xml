<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="portal_my_home_event" inherit_id="portal.portal_my_home" priority="90">
        <xpath expr="//div[hasclass('o_portal_category')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="icon" t-value="'/edit_at_portal_level_task/static/src/img/form-svgrepo-com (1).svg'"/>
                <t t-set="title">Custom Form</t>
                <t t-set="url" t-value="'/custom/form'"/>
                <t t-set="text">Create, Update Delete Your Form Data</t>
                <t t-set="config_card" t-value="True"/>
            </t>
        </xpath>
    </template>

</odoo>
